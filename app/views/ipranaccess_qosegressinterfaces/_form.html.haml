= form_for @ipranaccess_qosegressinterface do |f|
  - if @ipranaccess_qosegressinterface.errors.any?
    #error_explanation
      %h2= "#{pluralize(@ipranaccess_qosegressinterface.errors.count, "error")} prohibited this ipranaccess_qosegressinterface from being saved:"
      %ul
        - @ipranaccess_qosegressinterface.errors.full_messages.each do |message|
          %li= message

  .field
    = f.label :device
    = f.text_field :device
  .field
    = f.label :sap
    = f.text_field :sap
  .field
    = f.label :queueId
    = f.number_field :queueId
  .field
    = f.label :bps_max
    = f.text_field :bps_max
  .field
    = f.label :discard
    = f.text_field :discard
  .field
    = f.label :discard_avg
    = f.text_field :discard_avg
  .field
    = f.label :time
    = f.date_select :time
  .field
    = f.label :comment
    = f.text_area :comment
  .field
    = f.label :weeks
    = f.number_field :weeks
  .field
    = f.label :device_sap
    = f.text_field :device_sap
  .field
    = f.label :device_sapqueue
    = f.text_field :device_sapqueue
  .actions
    = f.submit 'Save'
